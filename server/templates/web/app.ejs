<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title><%- title %></title>
    <link rel="icon" type="image/png" href="/favicon.png" />
    <link rel="stylesheet" href="/vendors/bootstrap/css/bootstrap.min.css">
    <script src="/vendors/jquery/jquery-3.2.1.min.js"></script>
    <script src="/vendors/bootstrap/js/bootstrap.min.js"></script>
    <meta name="description" content="An infrastructure monitoring system for FusionOps">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    
    <link rel="stylesheet" href="/css/material-design-icons/iconfont/material-icons.css" rel="stylesheet" >
    
    <script type="text/javascript">
      window.GAW = {
        preloadedState: <%- JSON.stringify(preloadedState) %>
      };
    </script>
    <script type="text/javascript">
      WebFontConfig = {
        google: { families: [ 'Roboto:100,300,400,400italic,500,700:latin' ] }
      };
      (function() {
        var wf = document.createElement('script');
        wf.src = '/vendors/webfontloader/webfontloader.js';
        wf.type = 'text/javascript';
        wf.async = 'true';
        var s = document.getElementsByTagName('script')[0];
        s.parentNode.insertBefore(wf, s);
      })(); 
    </script>
    <style media="screen">
      html {
        font-family: 'Roboto', sans-serif;
        font-size: 62.5%;
      }

      body {
        line-height: 1.5;
        padding: 0;
        margin: 0;
      }
      
      h1 {
        font-weight: 300;
      }
    </style>
    
  </head>
  
  <body id="body">
    <section id="app-container">
    <div id="root">
      <%- html %>
    </div>
    </section>
    <script>__REACT_DEVTOOLS_GLOBAL_HOOK__ = parent.__REACT_DEVTOOLS_GLOBAL_HOOK__</script>
    
    <% if (manifest['vendor.js']) { %>
      <script src="/<%- manifest['vendor.js'] %>"></script>
    <% } %>
    <script src="/<%- manifest['app.js'] %>"></script>
  </body>
</html>
